{
  "name": "triple-quiz-angular",
  "version": "1.0.2",
  "author": "Karol Jaskółka",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:clear": "npx jest --clearCache",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint-staged:quiet": "lint-staged --quiet",
    "lint-staged:commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint"
    ],
    "*.{ts,js,json,scss,html,md}": [
      "prettier --write"
    ]
  },
  "pre-commit": [
    "lint-staged:commit",
    "test",
    "build"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.4",
    "@angular/common": "^14.2.4",
    "@angular/compiler": "^14.2.4",
    "@angular/core": "^14.2.4",
    "@angular/forms": "^14.2.4",
    "@angular/platform-browser": "^14.2.4",
    "@angular/platform-browser-dynamic": "^14.2.4",
    "@angular/router": "^14.2.4",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@ngrx/effects": "^14.3.1",
    "@ngrx/store": "^14.3.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.1",
    "@angular-devkit/build-angular": "^14.2.4",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.4",
    "@angular/compiler-cli": "^14.2.4",
    "@ngneat/spectator": "^11.2.1",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.2"
  },
  "engines": {
    "node": ">= 16.16.0",
    "npm": ">= 8.17.0"
  }
}
